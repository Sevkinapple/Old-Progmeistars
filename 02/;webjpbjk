<html>
<head>
<script type="text/javascript">
var a;
a = 1;

switch (a) {
	case 1: {
		console.log('->a=1');
		break;
	}
	case 2: {
		console.log('->a=2');
		break;
	}
	case 3: {
		console.log('->a=3');
		break;
	}
	default: {
		console.log('smthng went wrong');
		break;
	}
}

a = 2;

if (a == 1) {
	console.log('->a=1');
} else if (a == 2) {
	console.log('->a=2');
} else if (a == 3) {
	console.log('->a=3');
}
</script>
</head>
<body>
</body>
</html>
